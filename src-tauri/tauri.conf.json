{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": {
      "resources/": "./"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Clipture",
  "mainBinaryName": "clipture",
  "version": "0.0.0",
  "identifier": "me.sshcrack.clipture",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Bootstrap",
        "label": "bootstrap",
        "center": true,
        "closable": false,
        "resizable": false,
        "width": 300,
        "height": 150,
        "maximizable": false,


        "decorations": true,
        "url": "src/bootstrap.html"
      },
      {
        "title": "Clipture",
        "label": "main",
        "width": 800,
        "height": 600,
        "visible": false,
        "url": "src/main.html"
      }
    ]
  }
}